{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2016",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "./types"
  ]
}
